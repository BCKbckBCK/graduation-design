<body class="user-select">
    <header class="header">
        <nav class="navbar navbar-default" id="navbar">
            <div class="container">
                <div class="header-topbar hidden-xs link-border">
                    <ul class="site-nav topmenu">

                    </ul> 勤记录 懂分享
                </div>
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#header-navbar" aria-expanded="false"> <span class="sr-only"></span> <span class="icon-bar"></span> <span class="icon-bar"></span> <span class="icon-bar"></span> </button>
                    <h1 class="logo hvr-bounce-in">
                        <a href="#" title="木庄网络博客"><img src="" alt="图标" /></a>
                    </h1>
                </div>

                <div class="collapse navbar-collapse" id="header-navbar">
                    <form class="navbar-form visible-xs" action="/Search" method="post">
                        <div class="input-group">
                            <input type="text" name="keyword" class="form-control" placeholder="请输入关键字" maxlength="20" autocomplete="off" />
                            <span class="input-group-btn"> <button class="btn btn-default btn-search" name="search" type="submit">搜索</button> </span>
                        </div>
                    </form>
                    <ul class="nav navbar-nav navbar-right">
                        <%    if( session){%>

                            用户已登录
                            <li><a href="<%=HTTP_ROOT%>/quit">退出登录</a>
                            </li>

                            <%}; if(!session){ %>

                                <li><a href="<%=HTTP_ROOT%>/login">用户登录</a></li>
                                <%}%>
                                    <li><a href="<%=HTTP_ROOT%>/admin/login">管理员登录</a></li>


                    </ul>
                </div>

            </div>
        </nav>
    </header>s
    <section class="container">
        <div class="content-wrap">
            <div class="content">
                <div id="focusslide" class="carousel slide" data-ride="carousel">
                    <ol class="carousel-indicators">
                        <%banners.forEach((banner,index)=>{%>
                            <li data-target="#focusslide" data-slide-to="<%=index%>" <%if(index==0){%>class="active"
                                <%}%>></li>
                            <%})%>
                    </ol>
                    <div class="carousel-inner" role="listbox">
                        <%banners.forEach((banner,index)=>{%>
                            <div class="item <%if(index==0){%>active<%}%>">
                                <a href="<%=banner.href%>" target="_blank" title="<%=banner.title%>">
            <img src="<%=HTTP_ROOT%>/upload/<%=banner.src%>" alt="<%=banner.title%>" class="img-responsive" />
          </a>
                            </div>
                            <%})%>
                    </div>
                    <a class="left carousel-control" href="#focusslide" role="button" data-slide="prev" rel="nofollow"> <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span> <span class="sr-only">上一个</span> </a>
                    <a class="right carousel-control" href="#focusslide" role="button" data-slide="next" rel="nofollow"> <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span> <span class="sr-only">下一个</span> </a>
                </div>
                <article class="excerpt-minic excerpt-minic-index">
                    <h2><span class="red">【推荐】</span><a target="_blank" href="#" title="用DTcms做一个独立博客网站（响应式模板）">通知公告</a> </h2>
                    <p class="note">通知公告</p>
                </article>
                <div class="title">
                    <h3>最新发布</h3>
                    <div class="more">
                        <%catalogs.forEach(catalog=>{%>
                            <a href="<%=HTTP_ROOT%>/list/<%=catalog.ID%>" title="<%=catalog.title%>">
                                <%=catalog.title%>
                            </a>
                            <%})%>
                    </div>
                </div>

                <%articles.forEach(article=>{%>




                    <article class="excerpt excerpt-1" style="">
                        <a class="focus" href="<%=HTTP_ROOT%>/articledisplay/<%=article.ID%>" title="" target="_blank"><img class="thumb"  src="<%=HTTP_ROOT%>/upload/<%=article.list_img_src%>" alt="用DTcms做一个独立博客网站（响应式模板）" style="display: inline;" /></a>
                        <header>
                            <a class="cat" href="<%=HTTP_ROOT%>/articledisplay/<%=article.ID%>" title="">详情<i></i></a>
                            <h2>
                                <a href="<%=HTTP_ROOT%>/articledisplay/<%=article.ID%>" title="">
                                    <%=article.title%>
                                </a>
                            </h2>
                        </header>
                        <p class="meta"> <time class="time"><i class="glyphicon glyphicon-time"></i> <%=article.created_time%></time> <span class="views"><i class="glyphicon glyphicon-eye-open"></i> <%=article.view%></span>
                            <a class="comment" href="##comment" title="评论"><i class="glyphicon glyphicon-comment"></i> <%=article.comment%></a>
                        </p>
                        <p class="note">
                            <%=article.summary%>
                        </p>
                    </article>
                    <%})%>





            </div>
        </div>
        <div class="tlinks">
            Collect from
            <a href="http://www.cssmoban.com/">企业网站模板</a>
        </div>











        <aside class="sidebar">
            <div class="fixed">
                <div class="widget widget-tabs">
                    <ul class="nav nav-tabs" role="tablist">
                        <li role="presentation" class="active"><a href="#notice" aria-controls="notice" role="tab" data-toggle="tab">统计信息</a></li>
                        <li role="presentation"><a href="#contact" aria-controls="contact" role="tab" data-toggle="tab">联系站长</a></li>
                    </ul>
                    <div class="tab-content">
                        <div role="tabpanel" class="tab-pane contact active" id="notice">
                            <h2>资源总数:
                                <%=articles2.length%>篇 </h2>
                            <h2>网站运行: <span id="sitetime"><%=c%>天 </span></h2>
                        </div>
                        <div role="tabpanel" class="tab-pane contact" id="contact">
                            <h2>QQ:2788385944
                                <a href="" target="_blank" rel="nofollow" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="QQ:"></a>
                            </h2>
                            <h2>Email:2788385944@qq.com
                                <a href="#" target="_blank" data-toggle="tooltip" rel="nofollow" data-placement="bottom" title="" data-original-title="#"></a>
                            </h2>
                        </div>
                    </div>
                </div>
                <div class="widget widget_search">
                    <form class="navbar-form" action="/search" method="post">
                        <div class="input-group">
                            <input type="text" name="keyword" class="form-control" size="35" placeholder="请输入关键字" maxlength="15" autocomplete="off" />
                            <span class="input-group-btn"> <button class="btn btn-default btn-search"  name="search" type="submit">搜索</button> </span>
                        </div>
                    </form>
                </div>
            </div>
            <div class="widget widget_hot">
                <h3>最多评论文章</h3>
                <ul>
                    <%articles1.forEach(article=>{%>


                        <li>
                            <a title="<%=article.title%>" href="<%=HTTP_ROOT%>/articledisplay/<%=article.ID%>">
                                <span class="thumbnail"> <img class="thumb" src="<%=HTTP_ROOT%>/upload/<%=article.list_img_src%>" alt="<%=article.title%>" style="display: block;" /> </span>
                                <span class="text"><%=article.title%></span><span class="muted"><i class="glyphicon glyphicon-time"></i><%=article.created_time%></span>
                                <span class="muted"><i class="glyphicon glyphicon-comment"></i><%=article.comment%></span></a>
                        </li>
                        <% });%>
                </ul>
            </div>

            <div class="widget widget_sentence">
                <h3>友情链接</h3>
                <div class="widget-sentence-link">
                    <a href="https://jq.qq.com/?_wv=1027&k=5QlTyzL" target="_blank">点击加入it资源分享群</a>&nbsp;&nbsp;&nbsp;

                </div>
            </div>
        </aside>
    </section>
    <footer class="footer">
        <div class="container">
            <p>所有资源来自网上，如有侵权请联系站长</p>
        </div>

    </footer>
    <script src="js/bootstrap.min.js"></script>
    <script src="js/jquery.ias.js"></script>
    <script src="js/scripts.js"></script>
</body>

</html>